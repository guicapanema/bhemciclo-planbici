        "popper.js": "^1.14.3",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "aos": "^2.3.3",
        "chart.js": "^2.7.2",
        "dayjs": "^1.7.4",
        "moment": "^2.22.2",
        "rss-parser": "^3.4.2",
        "vue-chartjs": "^3.3.2"
    }
